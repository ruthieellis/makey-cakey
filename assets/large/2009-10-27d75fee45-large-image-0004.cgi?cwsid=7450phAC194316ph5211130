jQuery(document).ready(function(){
    var flagLang = false;
    jQuery('.form-language').hover(function(){
        if(flagLang==false){
            jQuery('#select-language').fadeIn(100);
            jQuery('#language-selector').addClass('active');
            flagLang = true;
        }
        else{
            jQuery('#select-language').fadeOut(100);
            jQuery('#language-selector').removeClass('active');
            flagLang = false;
        }
    });	
});
