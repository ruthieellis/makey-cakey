    jQuery(document).ready(function(){

        var saveNL = new DnDBilling('#is-header-subscribed', '.nl-subscriber', '#newsletter-header-action', '.nl-button', '#newsletter-validate-detail');

        // Stop form submit
        Event.observe('newsletter-validate-detail','submit',function(event) {
