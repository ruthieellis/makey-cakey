    <script type="text/javascript">
        jQuery(document).ready(function() {

            var _panelClass   = '.newsletter-box',
                _triggerClass = "#newsletter-link",
                _panel        = jQuery(_panelClass),
                _trigger      = jQuery(_triggerClass),
                _close        = jQuery('.close-newsletter-box'),
                _isHide       = true
                _slideDelay   = 150;


            function togglePanel(_force)
            {
                if(typeof _force != 'undefined') {
                    switch(_force) {
                        case 'show':
                            _showPanel();
                            break;
                        case 'hide':
                            _hidePanel();
                            break;
                    }
                }
                else {
                    if(_isHide === true) {
                        _showPanel();
                    }
                    else {
                        _hidePanel();
                    }
                }
            }

            function _showPanel()
            {
                _panel.stop(true,true).slideDown(_slideDelay, function() {
                    jQuery('#newsletter').focus();
                });
                _isHide = false;
            }

            function _hidePanel()
            {
                _panel.stop(true,true).slideUp(_slideDelay);
                _isHide = true;
            }

            _trigger.click(function(event) {
                event.preventDefault();
                togglePanel();
            });

            _close.click(function(event) {
                event.preventDefault();
                togglePanel('hide');
            });

            jQuery('body').click(function(event) {
                var _child = jQuery(event.target);
                if(!_child.parents(_panelClass).length && !_child.is(_triggerClass) && !_child.is(_panelClass)) {
                    togglePanel('hide');
                }
            });

        });
    </script>

