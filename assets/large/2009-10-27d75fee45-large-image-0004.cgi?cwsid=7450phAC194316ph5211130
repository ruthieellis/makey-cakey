<script>
    /*Script dropdown account link*/
    jQuery(document).ready(function(){
        var dropdown = false;
        jQuery('.account-box').hover(function(){
            if(dropdown==false){
                jQuery('.account-informations').fadeIn(100).addClass('active');
                jQuery('.account-login').addClass('active');
                dropdown = true;
            }
            else{
                jQuery('.account-informations').fadeOut(100).removeClass('active');
                jQuery('.account-login').removeClass('active');
                dropdown=false;
            }
        });
    });
</script>


